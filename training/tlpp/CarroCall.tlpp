  obj:SetCor("Vermelho")
  obj:SetAno(2000)
  
Return obj

User Function CarroEletrico()
  Local obj as Object
  obj:= ElectrickCar():New(.T.)
  obj:SetModelo("Tesla")
  obj:SetCor("Preto")
  obj:SetAno(2020)
  obj:Ligar(.T.)
  obj:Ligado(.T.)
  obj:CarregarBateria(100)
  
Return obj
  